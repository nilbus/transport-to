{
  "manifest_version": 3,
  "name": "Open in Comet",
  "version": "1.0",
  "description": "Opens the current page in Comet browser",
  "key": "",
  "permissions": [
    "activeTab",
    "nativeMessaging"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "32": "comet.png"
    },
    "default_title": "Open in Comet"
  },
  "icons": {
    "32": "comet.png"
  }
}
